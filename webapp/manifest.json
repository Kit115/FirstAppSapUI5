{
	"_version": "1.21.0",
	"sap.app": {
		"id": "sap.ui.selfmade.appOne",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"employeeRemote": {
				"uri": "/here/goes/your/serviceUrl/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri" : "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.selfmade.appOne.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.selfmade.appOne.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"productModel": {
                "type" : "sap.ui.model.json.JSONModel",
                "uri" : "models/Products.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config" : {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.selfmade.appOne.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes" : [
				{
					"pattern": "", 
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "detail/{productPath}",
					"name": "detail", 
					"target": "detail"
				}
			],
			"targets" : {
				"home" : {
					"viewId": "home",
					"viewName": "Home"
				},
				"detail" : {
					"viewId": "detail",
					"viewName": "Detail"
				}
			}
		}
	}
}
